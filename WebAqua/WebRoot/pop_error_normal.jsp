<%@ page contentType="text/html;charset=euc-kr" %><%@ page import="org.apache.struts.util.RequestUtils" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%	String errorCode = (String)request.getAttribute("ERROR_CODE");//	String returnURL = (String)request.getAttribute("RETURN_URL");	String errorString = RequestUtils.message(pageContext, null, null, errorCode);	String errorTitle = "";	String errorContent = "";	String errorMessage = "";	if(request.getAttribute("ERROR_MESSAGE") != null) {		errorMessage = (String)request.getAttribute("ERROR_MESSAGE");	}
	if(errorString != null && errorString.length() > 0) {		int barIndex =  errorString.indexOf('|');		errorTitle = errorString.substring(0,barIndex);		errorContent = errorString.substring(barIndex+1);	}%><html><head><title><bean:message key="index.title"/></title><meta http-equiv="Content-Type" content="text/html; charset=euc-kr"><link rel="stylesheet" href="/iss/css/txt.css" type="text/css"></head><body bgcolor="#FFFFFF" text="#000000" leftmargin="0" topmargin="0"><br><br><br><table width="540" border="0" align="center" cellpadding="0" cellspacing="0">  <tr>     <td width="180" rowspan="2"><div align="center"><img src="/img/error_p.gif" width="103" height="105"></div>      <img src="/img/error_p1.gif" width="180" height="40"></td>    <td width="380" height="10"><div align="center"></div></td>  </tr>  <tr>     <td width="380"><table width="330" border="0" align="right" cellpadding="0" cellspacing="5">        <tr>           <td height="25"><strong><font color="#999900">[<%= errorTitle %>]</font></strong>           </td>        </tr>        <tr>           <td height="25"><%= errorContent %>(<%= errorCode %>)<br><%= errorMessage %></td>        </tr>        <tr>           <td height="15"><img src="/img/dot_01.gif" width="9" height="9"> 자세한             사항은 관리자에게 문의해 주십시오. </td>        </tr>        <tr>                  </tr>      </table>      <div align="center"></div></td>  </tr>  <tr>     <td width="180">&nbsp;</td>    <td width="380"><div align="center"><a href="#" onfocus="this.blur();" style="border:0" onClick="self.close();"><img src="/img/close.gif" width="54" height="23" border="0"></a></div></td>  </tr></table><p>&nbsp;</p></body></html>