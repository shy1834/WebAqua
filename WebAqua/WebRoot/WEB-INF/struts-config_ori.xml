<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<!--
  Default configuration file for examples application.
  Each module also has its own struts-config under: /WEB-INF/$MODULE/
  @version $Revision: 1.1 $ $Date: 2011/03/05 06:02:23 $
-->
<struts-config>
  <data-sources />

  <form-beans>  
<!--form-bean name="menuForm" type="com.ktf.aqua.mgr.menu.common.MenuForm"/-->		

<!-- 품질 분석 출력 관련 -->
	<form-bean name="mainForm" type="org.apache.struts.action.DynaActionForm">
		<form-property name="cmd" type="java.lang.String" initial="" />
	</form-bean>

	<form-bean name="userInfoForm" type="org.apache.struts.action.DynaActionForm">
		<form-property name="cmd" type="java.lang.String" initial="" />
		<form-property name="userId" type="java.lang.String" initial="" />
	</form-bean>

<!-- Report 출력 관련 -->
		<form-bean name="ReportTemplateForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="rept_tmpl_file" type="org.apache.struts.upload.FormFile" />
			<form-property name="rept_tmpl_nm" type="java.lang.String" />
			<form-property name="rept_prd" type="java.lang.String" />
			<form-property name="rept_tmpl_id" type="java.lang.String" />
			<form-property name="rept_stat_id" type="java.lang.String[]" />
			<form-property name="start_row" type="java.lang.String[]" />
			<form-property name="start_col" type="java.lang.String[]" />
			<form-property name="end_row" type="java.lang.String[]" />
			<form-property name="end_col" type="java.lang.String[]" />
			<form-property name="sql" type="java.lang.String[]" />
			<form-property name="data_type" type="java.lang.String[]" />
			<form-property name="sheet_no" type="java.lang.String[]" />
		</form-bean>
		
  </form-beans> 

  <global-exceptions>

<!-- 품질 분석 출력 관련 -->
  <exception key="ABSTRACT" type="java.lang.Exception" path="/main/error.jsp"/>

  </global-exceptions> 

  <global-forwards>
  <!-- utilize a custom ActionForward as an example only -->

<!-- 품질 분석 출력 관련 -->
    <forward   name="logoff"               path="/login/login.jsp"/>
    <forward   name="logon"                path="/login/logout.jsp"/> 

  </global-forwards>
  
<action-mappings>

<!-- 품질 분석 액션 -->
	<!-- reload -->
    <action path="/reload" type="com.ktf.aqua.action.ReloadStrutsConfigAction">
	</action>	
 
	<action path="/login"
               type="com.ktf.aqua.user.action.UserLoginAction"
               name="mainForm"
               parameter="cmd"
               scope="request"
               validate="false">
      <forward name="login"           path="/login/login.jsp" redirect="false" />
      <forward name="logout"          path="/login/logout.jsp" redirect="false" />	 
	  <forward name="authority"       path="/login/UserAuthCheck.jsp" redirect="false" />
    </action>	

	<action path="/welcome" type="com.ktf.aqua.welcome.action.WelcomeAction"
               name="mainForm"
               parameter="cmd"
               scope="request"
               validate="false">
      <forward name="goMain"        path="/admin.jsp"       		redirect="false" />
    </action>

	<action path="/mainPage" type="com.ktf.aqua.mgr.menu.action.MainPageAction">
      <forward name="default"        path="/admin/main.jsp"         redirect="false" />
      <forward name="default_cp"     path="/cp/admin/main.jsp"      redirect="false" />
      <forward name="notAllowed"     path="/login_error.jsp"        redirect="false" />
    </action>
    
    <action path="/boardInfo" type="com.ktf.aqua.user.action.BbsInfoAction">
      <forward name="bbsInfo"      path="/board/view_pop.jsp"     redirect="false" />
    </action>

	<action path="/menu" type="com.ktf.aqua.mgr.menu.action.MenuListAction">
      <forward name="tcp"      		path="/tstat/tcp.jsp"			redirect="false" />
      <forward name="http"    		path="/tstat/http.jsp"			redirect="false" />
      <forward name="maiden"        path="/tstat/maiden.jsp"		redirect="false" />
      <forward name="page"     		path="/tstat/page.jsp"			redirect="false" />
      <forward name="issue"			path="/tstat/issue.jsp"			redirect="false" />
      <forward name="rtsp"			path="/tstat/rtsp.jsp"			redirect="false" />
      <forward name="vod"			path="/tstat/vod.jsp"			redirect="false" />
      <forward name="onLine"		path="/tstat/onLine.jsp"		redirect="false" />
      <forward name="gipyo"			path="/tstat/gipyo.jsp"			redirect="false" />
      <forward name="errDetail"		path="/tstat/errDetail.jsp"		redirect="false" />
    </action>

    <action path="/userInfo" type="com.ktf.aqua.user.action.UserInfoAction">
      <forward name="userInfo" 		path="/admin/user_info.jsp"		redirect="false" />
      <forward name="userView" 		path="/admin/user_view.jsp"		redirect="false" />
      <forward name="userEdit" 		path="/admin/user_edit.jsp"		redirect="false" />
      <forward name="userEdit1" 	path="/admin/user_edit.jsp"		redirect="false" />
      <forward name="userDelete" 	path="/admin/user_delete.jsp"	redirect="false" />
      <forward name="userSearch" 	path="/admin/user_search.jsp"	redirect="false" />
      <forward name="userList" 		path="/admin/user.jsp"			redirect="false" />
      <forward name="menuList" 		path="/admin/menu.jsp"			redirect="false" />
      <forward name="adminList"		path="/admin/admin.jsp"			redirect="false" />
      <forward name="dataMstList"	path="/admin/data_mst.jsp"		redirect="false" />
      <forward name="svcMstList"	path="/admin/svc_mst.jsp"		redirect="false" />
      <forward name="errMstList"	path="/admin/err_mst.jsp"		redirect="false" />
      <forward name="regPopList"	path="/admin/reg_pop.jsp"		redirect="false" />
      <forward name="userLog"		path="/admin/user_log.jsp"		redirect="false" />
      <forward name="gongjiBoard" 	path="/board/gongji_list.jsp" 	redirect="false" />
      <forward name="userBoard" 	path="/board/user_list.jsp"		redirect="false" />
    </action>

<!-- Report 액션 -->	
	<action path="/daily_report/rept_tmpl_inquiry" type="com.ktf.iss.report.ReportTemplateInquiry_ACT" validate="false" scope="request" >
		<forward name="ReptTmplInquiry" path="/daily_report/rept_tmpl_inquiry.jsp" redirect="false" />
	</action>
	<action path="/daily_report/rept_tmpl_inquiry_cus" type="com.ktf.iss.report.ReportTemplateInquiry_ACT" validate="false" scope="request" >
		<forward name="ReptTmplInquiry" path="/daily_report/rept_tmpl_inquiry_cus.jsp" redirect="false" />
	</action>
	<action path="/daily_report/rept_tmpl_regform" forward="/daily_report/rept_tmpl_regform.jsp"/>
	
	<action path="/daily_report/rept_tmpl_register" type="com.ktf.iss.report.ReportTemplateCreate_ACT" name="ReportTemplateForm"  validate="false" scope="request" >
		<forward name="ReptTmplInquiry" path="/daily_report/rept_tmpl_inquiry.do" redirect="true" />
	</action>
	<action path="/daily_report/rept_tmpl_delete" type="com.ktf.iss.report.ReportTemplateDelete_ACT" validate="false" scope="request" >
		<forward name="ReptTmplInquiry" path="/daily_report/rept_tmpl_inquiry.do" redirect="true" />
	</action>
	<action path="/daily_report/rept_tmpl_modform" type="com.ktf.iss.report.ReportTemplateModifyForm_ACT" name="ReportTemplateForm" validate="false" scope="request" >
		<forward name="ReptTmplModify" path="/daily_report/rept_tmpl_modform.jsp" redirect="false" />
	</action>
	<action path="/daily_report/rept_tmpl_modify" type="com.ktf.iss.report.ReportTemplateModify_ACT" name="ReportTemplateForm" validate="false" scope="request" >
		<forward name="ReptTmplInquiry" path="/daily_report/rept_tmpl_inquiry.do" redirect="true" />
	</action>
	<action path="/daily_report/rept_create_form" type="com.ktf.iss.report.ReportCreateForm_ACT" validate="false" scope="request" >
		<forward name="ReptCreate" path="/daily_report/rept_create_form.jsp" redirect="false" />
	</action>
	<action path="/daily_report/rept_create_form_cus" type="com.ktf.iss.report.ReportCreateForm_ACT" validate="false" scope="request" >
		<forward name="ReptCreate" path="/daily_report/rept_create_form_cus.jsp" redirect="false" />
	</action>
	<action path="/daily_report/rept_create" type="com.ktf.iss.report.ReportCreate_ACT" validate="false" scope="request" >
		<forward name="ReptInquiry" path="/daily_report/rept_inquiry.do" redirect="true" />
	</action>
	<action path="/daily_report/rept_create_cus" type="com.ktf.iss.report.ReportCreate_ACT" validate="false" scope="request" >
		<forward name="ReptInquiry" path="/daily_report/rept_inquiry_cus.do" redirect="true" />
	</action>
	<action path="/daily_report/rept_delete" type="com.ktf.iss.report.ReportDelete_ACT" validate="false" scope="request" >
		<forward name="ReptInquiry" path="/daily_report/rept_inquiry.do" redirect="true" />
	</action>
	<action path="/daily_report/rept_download" type="com.ktf.iss.report.ReportDownload_ACT" validate="false" scope="request" />
	
	<action path="/daily_report/rept_inquiry" type="com.ktf.iss.report.ReportInquiry_ACT" validate="false" scope="request" >
		<forward name="ReptInquiry" path="/daily_report/rept_inquiry.jsp" redirect="false" />
	</action>
	<action path="/daily_report/rept_inquiry_cus" type="com.ktf.iss.report.ReportInquiry_ACT" validate="false" scope="request" >
		<forward name="ReptInquiry" path="/daily_report/rept_inquiry_cus.jsp" redirect="false" />
	</action>
	<action path="/daily_report/pop_table_inquiry" type="com.ktf.iss.report.TableInquiry_ACT" name="TableSearchForm" validate="false" scope="request" >
		<forward name="TableInquiry" path="/daily_report/pop_table_inquiry.jsp" redirect="false" />
	</action>

  </action-mappings>


  <message-resources parameter="MessageResources" />

  <message-resources null="false" parameter="com.ktf.iss.ApplicationResources" />

  <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
		              value="/WEB-INF/validator-rules.xml,/WEB-INF/validator.xml"/>
	</plug-in>

</struts-config>
