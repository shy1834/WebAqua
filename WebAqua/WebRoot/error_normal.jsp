<%@ page contentType="text/html;charset=euc-kr" %><%@ page import="org.apache.struts.util.RequestUtils" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%	String errorCode = (String)request.getAttribute("ERROR_CODE");	String returnURL = (String)request.getAttribute("RETURN_URL");	String errorString = RequestUtils.message(pageContext, null, null, errorCode);	String errorTitle = "";	String errorContent = "";	String errorMessage = "";	if(request.getAttribute("ERROR_MESSAGE") != null) {		errorMessage = (String)request.getAttribute("ERROR_MESSAGE");	}		if(errorString != null && errorString.length() > 0) {		int barIndex =  errorString.indexOf('|');		errorTitle = errorString.substring(0,barIndex);		errorContent = errorString.substring(barIndex+1);	} %><html><head><title><bean:message key="index.title"/></title><meta http-equiv="Content-Type" content="text/html; charset=euc-kr"><link rel="stylesheet" href="/css/txt.css" type="text/css"></head><body bgcolor="#FFFFFF" text="#000000" leftmargin="0" topmargin="0"><table width="857" border="0" cellspacing="0" cellpadding="0">  <tr>     <td><br>      <br>      <br>      <br>      <br>      <br>      <br>      <br>      <br>      <TABLE WIDTH=605 BORDER=0 align="center" CELLPADDING=0 CELLSPACING=0>        <TR>           <TD> <IMG SRC="/img/wt_01.gif" WIDTH=43 HEIGHT=41 ALT=""></TD>          <TD width="50" rowspan="4"><img src="/img/error.gif" width="181" height="171">           </TD>          <TD> <IMG SRC="/img/wt_04.gif" WIDTH=386 HEIGHT=41 ALT=""></TD>          <TD> <IMG SRC="/img/wt_05.gif" WIDTH=34 HEIGHT=41 ALT=""></TD>        </TR>        <TR>           <TD>&nbsp; </TD>          <TD> <IMG SRC="/img/wt_23.gif" WIDTH=386 HEIGHT=42 ALT=""></TD>          <TD> <IMG SRC="/img/wt_10.gif" WIDTH=34 HEIGHT=42 ALT=""></TD>        </TR>        <TR>           <TD>&nbsp; </TD>          <TD valign="middle"><table width="386" border="0" cellspacing="0" cellpadding="0">              <tr>                 <td width="62">&nbsp;</td>                <td width="322">                   <table width="330" border="0" cellspacing="5" cellpadding="0">                    <tr>                       <td height="25"><strong><font color="#999900">[<%= errorTitle %>]</font></strong>                       </td>                    </tr>                    <tr>                      <td height="25"><%= errorContent %>(<%= errorCode %>)<br><%= errorMessage %></td>                    </tr>                    <tr>                       <td height="15"><img src="/img/dot_01.gif" width="9" height="9">                         자세한 사항은 관리자에게 문의해 주십시오. </td>                    </tr>                    <tr>                       <td height="15"><img src="/img/dot_01.gif" width="9" height="9">                         관리자 : <a href="mailto:<%=com.ktf.iss.system.SysUserInfo_BO.getInstance().getSysAdminInfo().getEmail() %>">어드민(<%= com.ktf.iss.system.SysUserInfo_BO.getInstance().getSysAdminInfo().getEmail() %>)</a>                       </td>                    </tr>                  </table> </td>              </tr>            </table></TD>          <TD>&nbsp; </TD>        </TR>        <TR>           <TD> <IMG SRC="/img/wt_16.gif" WIDTH=43 HEIGHT=37 ALT=""></TD>          <TD> <IMG SRC="/img/wt_19.gif" WIDTH=386 HEIGHT=37 ALT=""></TD>          <TD> <IMG SRC="/img/wt_20.gif" WIDTH=34 HEIGHT=37 ALT=""></TD>        </TR>        <TR>           <TD height="50" colspan="4"><% if(returnURL != null && !returnURL.equals("#")) { %><div align="center"><a href="<%= returnURL %>" onfocus="this.blur();" style="border:0"><img src="/img/sure.gif" width="54" height="23" border="0"></a></div><% } %></TD>        </TR>      </TABLE>          </td>  </tr></table><p>&nbsp;</p></body></html>